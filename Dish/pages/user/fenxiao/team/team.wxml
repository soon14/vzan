<!--pages/user/fen/team/team.wxml-->
<view class="teams tc flex">
  <view class="sub gridYr">
    <view class="f24">{{data_list.sevencount}}</view>
    <view class="f14 mt5 linght">最近七日新增</view>
  </view>
  <view class="sub gridYr">
    <view class="f24">{{data_list.monthcount}}</view>
    <view class="f14 mt5 linght">最近一月新增</view>
  </view>
  <view class="sub gridYr">
    <view class="f24">{{data_list.allcount}}</view>
    <view class="f14 mt5 linght">团队共有</view>
  </view>
</view>

<view class="tongji tc flex">
  <view class="sub">
    <view class="shiyi">
      <view class="zhu" style="height: {{data_list.firstper}}%; background: #f35150;">
        <view>{{data_list.firstcount}}人<text class="linght">({{data_list.firstper}}%)</text></view>
      </view>
    </view>
    <view class="mt10">一级分销</view>
  </view>
  <view class="sub">
    <view class="shiyi">
      <view class="zhu" style="height: {{data_list.secondper}}%; background: #fea512;">
        <view>{{data_list.secondcount}}人<text class="linght">({{data_list.secondper}}%)</text></view>
      </view>
    </view>
    <view class="mt10">二级分销</view>
  </view>
</view>

<navigator class="more" url="../teamlist/teamlist"><button type="default">查看所有成员</button></navigator>

