<!--pages/user/fen/index/index.wxml -->
<view class="fhome">
  <view class="overview grey tc f14">
    <view class="weeks">{{fxinfo.gold}}</view>
    <view>账户余额（元）</view>
  </view>

  <view class="ovtab tc flex">
    <view class="sub gridYr">
      <view class="f20">{{fxinfo.total_money}}</view>
      <view class="f14 linght">总收入</view>
    </view>
    <view class="sub gridYr">
      <view class="f20">{{fxinfo.wtotal_money}}</view>
      <view class="f14 linght">未到账收入</view>
    </view>
  </view>

  <view class="ftab">
    <navigator url="../record/record">
      <view class="ftab-item gridXt">
        <text class="arrowL">分销记录</text>
      </view>
    </navigator>
    <navigator url="url">
      <view class="ftab-item false gridXt">
        <view class="fr grey lineText">{{fxinfo.refername}}</view>
        <text class="arrowL">我的推荐人</text>
      </view>
    </navigator>
    <navigator url="../team/team">
      <view class="ftab-item gridXt">
        <text class="arrowL">我的团队</text>
      </view>
    </navigator>
    <navigator url="../qcode/qcode">
      <view class="ftab-item gridXt">
        <text class="arrowL">分销二维码</text>
      </view>
    </navigator>
    <!-- <navigator url="../withdraw/withdraw">
      <view class="ftab-item gridXt">
        <text class="arrowL">我要提现</text>
      </view>
    </navigator> -->
    <navigator url="../wrecord/wrecord">
      <view class="ftab-item gridXt">
        <text class="arrowL">提现记录</text>
      </view>
    </navigator>
  </view>

</view>