    <script type="text/javascript" src="@(WebSiteConfig.cdnurl)content/dlptcssapp/js/jquery.form.min.js"></script>
    <!--<script src="@(WebSiteConfig.cdnurl)content/dlptcssapp/js/bootstrap.js"></ script>-->
    <script>
        var mySwiper = new Swiper('.swiper-container', {
            direction: 'vertical',
            //是否循环滑动
            loop: false,

            paginationClickable: true,
            //显示视图个数
            slidesPerView: 1,

            //鼠标滑动切换
            mousewheelControl: true,

            // 如果需要分页器
            //		    pagination: '.swiper-pagination',
        })
        var entrance = document.getElementsByClassName("entrance");
        var entrance_centain = document.getElementsByClassName("entrance_centain");
        for (var i = 0; i < entrance.length; i++) {
            var p = entrance_centain[i].getElementsByTagName("p")[0];
            p.style.padding = (entrance_centain[i].offsetHeight - p.offsetHeight) / 2 + "px 0";
        }

        var support_contain = document.getElementsByClassName("support_contain");
        for (var i = 0; i < support_contain.length; i++) {
            var p = support_contain[i].getElementsByTagName("p")[0];
            p.style.padding = (support_contain[i].offsetHeight - p.offsetHeight) / 2 + "px 0";
        }

        var Text = document.getElementsByClassName("text");
        var Text_height = 0;
        if (Text[0].offsetHeight > Text[1].offsetHeight) {
            Text[1].style.height = Text[0].offsetHeight + "px"
        } else {
            Text[0].style.height = Text[1].offsetHeight + "px"
        }
        if (Text[2].offsetHeight > Text[3].offsetHeight) {
            Text[3].style.height = Text[2].offsetHeight + "px"
        } else {
            Text[2].style.height = Text[3].offsetHeight + "px"
        }

        var consulting1 = document.getElementsByClassName("consulting")[0];
        var consulting2 = document.getElementsByClassName("consulting_tem")[0];
        var consulting = document.getElementById("consulting");
        var consulting_form = document.getElementsByName("consulting_form")[0];
        var sub = document.getElementById("submit");
        var Username = document.getElementById("Username");
        var Phone = document.getElementById("Phone");
        var Source = document.getElementById("Source");
        var result = document.getElementById("result");
        consulting1.onclick = function () {
            consulting.style.display = "block";
            Phone.value = "";
            Username.value = "";
            consulting_form.style.marginTop = "-" + consulting_form.offsetHeight / 2 + "px";
            consulting_form.style.marginLeft = "-" + consulting_form.offsetWidth / 2 + "px";
        }
        consulting2.onclick = function () {
            consulting.style.display = "none"
        }

        function submitTest() {
            if (Username.value.length > 20 || Username.value == "" || Username.value == null) {
                result.innerHTML = "<span>请输入正确姓名</span>";
                result.style.display = "block";
                result.style.marginTop = "-" + result.offsetHeight / 2 + "px";
                result.style.marginLeft = "-" + result.offsetWidth / 2 + "px";
                setTimeout(function () {
                    result.style.display = "none";
                }, 1000)
                return false;
            }
            if (!(/^1[3|4|5|8][0-9]\d{4,8}$/.test(Phone.value)) || Phone.value == "" || Phone.value == null || Phone.value.length != 11) {
                result.innerHTML = "<span>请输入正确手机号</span>";
                result.style.display = "block";
                result.style.marginTop = "-" + result.offsetHeight / 2 + "px";
                result.style.marginLeft = "-" + result.offsetWidth / 2 + "px";
                setTimeout(function () {
                    result.style.display = "none";
                }, 1000)
                return false;
            }
            $.post("/dlpt/SendUserAdvisory", { Phone: Phone.value, username: Username.value, Source: Source.value }, function (data) {
                result.innerHTML = "<span>" + data.Msg + "</span>";
                result.style.display = "block";
                result.style.marginTop = "-" + result.offsetHeight / 2 + "px";
                result.style.marginLeft = "-" + result.offsetWidth / 2 + "px";
                if (data.isok) {
                    setTimeout(function () {
                        consulting.style.display = "none";
                        result.style.display = "none";
                    }, 2000)
                } else {
                    setTimeout(function () {
                        result.style.display = "none";
                    }, 1000)
                }
            });
        }
    </script>
