<script>
    var TypeIndex=@TypeIndex;
    var _layer = parent.layer;
    var templateid=@templateid;
    var aid=@appid;
    var goodtypelist=@Html.Raw(JsonConvert.SerializeObject(ptypes));
    var goodmintypelist=@Html.Raw(JsonConvert.SerializeObject(pmintypes));
    var _newstypelist=@Html.Raw(JsonConvert.SerializeObject(newstypes));
    var _news2typelist=@Html.Raw(JsonConvert.SerializeObject(news2types));
    var _goodexttypeslist=@Html.Raw(JsonConvert.SerializeObject(pexttypes));
    var pages = JSON.parse(@Html.Raw(JsonConvert.SerializeObject(pageModel.pages)));
    var meConfig = @Html.Raw(pageModel.MeConfigJson);
    if (pages && pages.length > 0) {
        pages.forEach(function (pageitem) {
            if (pageitem.coms && pageitem.coms.length > 0) {
                pageitem.coms.forEach(function (comitem) {
                    //给旧数据的轮播图增加新属性
                    if (comitem.type == "slider" || comitem.type == "imgnav" || comitem.type == "magicCube" || comitem.type == "bottomnav") {
                        var items = null;
                        if (comitem.items) {
                            items = comitem.items;
                        }
                        if (comitem.navlist) {
                            items = comitem.navlist;
                        }
                        if (items.length > 0) {
                            items.forEach(function (item) {
                                if (item.furl === undefined) {
                                    item.furl = "-1";
                                }
                                if (item.urltype === undefined) {
                                    if (item.url == -1 || item.url === "") {
                                        item.urltype = -1;
                                    }
                                    else {
                                        item.urltype = 0;
                                    }
                                }
                                if (item.appid === undefined) {
                                    item.appid = "";
                                }
                                if (item.path === undefined) {
                                    item.path = "";
                                }
                                if (item.target === undefined) {
                                    item.target = "_blank";
                                }
                                if (item.itemstype == undefined) {
                                    item.itemstype = []
                                }
                                if (item.items == undefined) {
                                    item.items = [];
                                }
                                if (item.btnType == undefined) {
                                    item.btnType = "";
                                }
                            });
                        }

                    }
                    //给旧的图片组件新增属性
                    else if (comitem.type == "img") {
                        if (comitem.furl === undefined) {
                            comitem.furl = "-1";
                        }
                        if (comitem.urltype === undefined) {
                            if (comitem.url == -1 || comitem.url === "") {
                                comitem.urltype = -1;
                            }
                            else {
                                comitem.urltype = 0;
                            }
                        }
                        if (comitem.appid === undefined) {
                            comitem.appid = "";
                        }
                        if (comitem.path === undefined) {
                            comitem.path = "";
                        }
                        if (comitem.target === undefined) {
                            comitem.target = "_blank";
                        }
                        if (comitem.itemstype == undefined) {
                            comitem.itemstype = []
                        }
                        if (comitem.items == undefined) {
                            comitem.items = [];
                        }
                        if (comitem.btnType == undefined) {
                            comitem.btnType = "";
                        }
                    }
                    else if (comitem.type == "form") {
                        if (comitem.formstyle === undefined) {
                            comitem.formstyle = 1;
                        }
                    }
                    else if (comitem.type == "good") {
                        if (comitem.items.length > 0) {
                            comitem.items.forEach(function (item) {
                                if (item.img === undefined) {
                                    item.img = "";
                                }
                            });
                        }
                        if (comitem.title === undefined) {
                            comitem.title = "热门推荐";
                        }
                    }
                    else if (comitem.type == "contactShopkeeper") {
                        if (comitem.txt === undefined) {
                            comitem.txt = "";
                        }
                        if (comitem.pageShow === undefined) {
                            comitem.pageShow = true;
                        }
                        if (comitem.iconShow === undefined) {
                            comitem.iconShow = false;
                        }
                        if (comitem.serverType === undefined) {
                            comitem.serverType = "wxServer";
                        }
                        if (comitem.iconType == undefined) {
                            comitem.iconType = 0;
                        }
                        if (comitem.iconUrl == undefined) {
                            comitem.iconUrl = "";
                        }
                    }
                    else if (comitem.type == "cutprice") {
                        if (comitem.title === undefined) {
                            comitem.title = "砍价活动";
                        }
                    }
                    else if (comitem.type == "joingroup") {
                        if (comitem.title === undefined) {
                            comitem.title = "团购活动";
                        }
                    }
                    else if (comitem.type == "entjoingroup") {
                        if (comitem.title === undefined) {
                            comitem.title = "拼团活动";
                        }
                    }
                    else if (comitem.type == "news") {
                        comitem.titlestyle = 3;
                    }
                });
            }
        });
    }
    var goodItem =@Html.Raw(JsonConvert.SerializeObject(new EntGoods()));
    var dirname = '@(dirName)';
    var pagetype=@(Model.PageType);
    var storeId=@storeId;
    var HomeId =@HomeId;
    var syncmainsite=@Html.Raw(JsonConvert.SerializeObject(pageModel.syncmainsite))===1;

    //额外的配置
    var extraConfig = @Html.Raw(JsonConvert.SerializeObject(Model.extraConfig));
    console.log("执行了");
</script>

