            <script>
                    $(function () {

                        $("#touxiang").hover(
                         function () {
                             $(".user-setting").show();
                         },
                         function () {
                             $(".user-setting").hide();
                         }
                     )


                        $(".versions-list>li>a").click(function () {
                            $(".versions-list>li>a").css("color", "").removeClass("current-blue");
                            $(this).css("color", "#3296FA").addClass("current-blue");
                        });

                        $(".btn-label").click(function () {
                            $(".btn-label").removeClass("btn-label-blue fp-font-white");
                            $(this).addClass("btn-label-blue fp-font-white");
                        })

                        $.post("/dzhome/GetUserCookie", {aid:'@ViewBag.appId',pageType:@ViewBag.PageType},function (data) {
                            if (data.isok) {
                                if (data.dataObj._Account != null) {
                                    $("#logined").show();
                                    $("#userloginid").html(data.dataObj._Account.LoginId);
                                    //判断是否开通了代理推广
                                    if(data.dataObj.IsDistributioner)
                                    {
                                        $("#distributionbox").show();
                                    }
                                    if (data.dataObj.Agentinfo != null) {
                                        $("#agentauthod").html(data.dataObj.Agentinfo.AuthCode);
                                        $("#showagent").show();

                                        //普通代理才有免费使用
                                        if(data.dataObj.Agentinfo.AgentType==0)
                                        {
                                            $("#experiencebtn").show();
                                        }

                                        //分销代理没有充过预存款则不显示体验
                                        if(data.dataObj.Agentinfo.IsAgentDistribution && data.dataObj.Agentinfo.LastDeposit<=0)
                                        {
                                            $("#experiencebtn").hide();
                                        }
                                    }
                                    else {
                                        $("#agentauthodbox").hide();
                                        if(@ViewBag.PageType==0)
                                        {
                                            $("#showagent").hide();

                                        }
                                        else{
                                            $("#showagent").show();
                                        }
                                    }
                                    if (data.dataObj._Account.ConsigneePhone == null || data.dataObj._Account.ConsigneePhone == undefined || data.dataObj._Account.ConsigneePhone.length <= 0 ) {
                                        $("#updatephone").attr("data-type", "1");
                                        $("#updatephone").html("去绑定");

                                        $("#phonetitle").html("绑定手机号");
                                        //判断是否已绑定手机号
                                        if (@ViewBag.PageType==0 && $("#casetemplate_bindphone") != undefined && $("#casetemplate_bindphone").val()== "1") {
                                            $("#phonetitle").html("绑定手机号码让账号更安全");
                                        }

                                        //判断是否是代理商
                                        if(data.dataObj.Agentinfo == null)
                                        {
                                            //判断是否是代理商开通的用户
                                            if(!data.dataObj.IsAgentCustomer)
                                            {
                                                show("updatephonediv", 0);
                                            }
                                        }
                                    }
                                    else {
                                        $("#updatephone").attr("data-type", "2");
                                        $("#updatephone").html("去修改");
                                        $("#phonetitle").html("修改手机号");
                                        $("#userphone").html(data.dataObj._Account.ConsigneePhone);
                                        $("#editphone").html(data.dataObj._Account.ConsigneePhone);
                                    }
                                }
                            }
                        })
                    });

                    $(function () {
                        if ('@ViewBag.title'.length !== 0) {
                            var jq = $("span:contains(@ViewBag.title)").closest("li");
                            jq.addClass("active open");
                            while (jq.length > 0) {
                                jq = jq.parent("ul").parent("li");
                                jq.addClass("active open");
                                jq.children("a").find(".arrow").addClass("open");
                            }
                        }
                        if ($('#CName_Hidden').val() != undefined && $('#CName_Hidden').val() != '') {
                            localStorage.cityName = $('#CName_Hidden').val();
                        }
                    });
            </script>
            <script>
                    // #region 轮询是否有新订单
                    function IsHaveNewOrder()
                    {
                        $.ajax({
                            type: "POST",
                            url: "/base/IsHaveNewOrder",
                            data:{aid:'@ViewBag.appId'},
                            success: function (data) {
                                if(data.isok)
                                {
                                    if(data.dataObj.voicepath.length>0)
                                    {
                                        document.getElementById("orderAlert").src=data.dataObj.voicepath;
                                    }

                                    document.getElementById("orderAlert").play();
                                    $("#orderalert").attr("href",data.dataObj.redie_url);
                                    layer.open({
                                        type: 1,
                                        title: "系统提示",
                                        fixed: true,
                                        offset: 'rb',
                                        content: '<div style="text-align: center;padding:20px 0;color:#ff0000;">'+data.dataObj.content+'<p style="margin-top:10px;"><a class="btn btn-success" href="javascript:go_new_order(\''+data.dataObj.redie_url+'\');">点击查看</a></p></div>',
                                        id: 'IS_NEW_ORDER_DIV',
                                        area: ['350px'],
                                        shade: 0, //不显示遮罩
                                    });
                                }
                            }
                        });
                    }
                    function go_new_order(url) {
                        layer.closeAll();
                        window.location.href=url;
                        //$("#orderalert").click();
                    }
                    setInterval(function(){
                        IsHaveNewOrder();
                    }, 10000);
                    // #endregion
                    //提交信息
                    function submitInformation() {
                        var name = $("#contacts").val();
                        var phone = $("#contactsPhone").val().trim();
                        var typeValue = 2;

                        if (name == "" || phone == "") {
                            layer.msg("请填写完整信息");
                            return;
                        }
                        if (!(/^1[34578]\d{9}$/.test(phone))) {
                            layer.msg("请输入正确的手机号");
                            return;
                        }

                        $.post("/DLPT/SendUserAdvisory",
                            { Phone: phone, username: name, datasource: typeValue },
                            function (data) {
                                if (data.isok) {
                                    layer.msg(data.Msg);
                                    var name = $("#contacts").val("");
                                    var phone = $("#contactsPhone").val("");
                                    setTimeout(function () {
                                        window.location.reload();
                                    }, 1000);
                                }
                            })
                    }

                    //进入代理商小程序后台
                    function login(gourl,key) {
                        var url = window.location.host;
                        var domain = ".vzan.com";
                        if (url == "www.xiaochengxu.com.cn") {
                            domain = '.xiaochengxu.com.cn';
                        }
                        $.cookie(key,$.cookie("agent_UserCookieNew"), {
                            expires: 7,
                            path: '/',
                            domain: domain
                        });
                        if(open!=undefined && open==1)
                        {
                            window.open(gourl);
                        }
                        else{
                            window.location.href = gourl;
                        }
                    }

                    function CommandCookie(gourl,key,id,open) {
                        $.ajax({
                            url: "/dzhome/CommandCookie",
                            type: "post",
                            data: { key: key,agentcustomerid:id},
                            dataType: "json",
                            success: function (data) {
                                if (data.isok) {
                                    key=data.cookiename;
                                    var url = window.location.host;
                                    var domain = "";
                                    if (url.indexOf("www.") != -1) {
                                        domain = url.replace("www.", ".");
                                    }
                                    else {
                                        domain = "." + url;
                                    }

                                    $.cookie(key, data.kvalue, {
                                        expires: 7,
                                        path: '/',
                                        domain: domain
                                    });
                                    if(key === 'dz_UserCookieNew'){
                                        $.cookie('masterAuth', data.auth, {
                                            expires: 7,
                                            path: '/',
                                            domain: domain
                                        });
                                        $.cookie('adminAuth', 'x', {
                                            expires: 7,
                                            path: '/',
                                            domain: domain
                                        });
                                    }
                                   
                                    if(open!=undefined && open==1)
                                    {
                                        window.open(gourl);
                                    }
                                    else{
                                        window.location.href = gourl;
                                    }
                                }
                                else {
                                    layer.msg(data.msg);
                                }
                            }
                        });
                    }

                    //免费体验
                    function Experience() {
                        parent.layer.open({
                            type: 2,
                            title: "免费体验",
                            shade: 0.3,
                            area: ['650px', '540px'], //宽高
                            content: '/dzhome/experience'
                        });
                    }
                    //更换账号
                    function changeUserInfo() {
                        parent.layer.open({
                            type: 2,
                            title: "换绑账号",
                            shade: 0.3,
                            area: ['650px', '650px'], //宽高
                            content: '/agentmanager/ChangeUserInfo?id=0'
                        });
                    }
            </script>
