@using Entity.MiniApp.User
@using BLL.MiniApp.Footbath;
@using Entity.MiniApp.Footbath

@{
    Layout = null;
    var controlName = ViewContext.RouteData.Route.GetRouteData(this.Context).Values["controller"];
    int storeId = Utility.IO.Context.GetRequestInt("storeId", 0);//门店ID
    int appId = Utility.IO.Context.GetRequestInt("appId", 0);
    //是否为总店
    bool isMainStore = true;
    switch ((int)ViewBag.PageType)
    {
        case (int)TmpType.小程序多门店模板:
            FootBath store =  FootBathBLL.SingleModel.GetModel(storeId);
            if (store == null || store.HomeId == 0)
            {
                isMainStore = true;
            }
            else
            {
                isMainStore = false;
            }
            break;
    }

    string manageType = Utility.IO.Context.GetRequest("manageType", "");

    string domain = Request.Url.Host;
    domain = domain.GetUnicodeDomain();

    if (domain.Contains(WebSiteConfig.DzWebSiteDomainExt))
    {
        //表示二级域名
        domain = domain.Replace(WebSiteConfig.DzWebSiteDomainExt, "");
    }
    if (domain.Contains("xn--"))
    {
        System.Globalization.IdnMapping idn = new System.Globalization.IdnMapping();

        domain = idn.GetUnicode(domain);
    }
    AgentWebSiteInfo agentWebSiteInfo = AgentWebSiteInfoBLL.SingleModel.GetModelByDomian(domain);
    SeoConfigModel seoConfig = new SeoConfigModel();
    if (agentWebSiteInfo != null && !string.IsNullOrEmpty(agentWebSiteInfo.seoConfig))
    {
        seoConfig = JsonConvert.DeserializeObject<SeoConfigModel>
    (agentWebSiteInfo.seoConfig);
        if (seoConfig != null)
        {
            agentWebSiteInfo.seoConfigModel = seoConfig;
        }
    }
    Request.RequestContext.RouteData.Values["agentInfo"] = agentWebSiteInfo;
}

