    <style>
        body { background-color: #f5f5f5; font-family: "微软雅黑"; }
        a { color: #337ab7; text-decoration: none; }
        /*分页样式*/
        .page { background: #fff; padding: 0 10px; margin-top: 12px; font-size: 12px; }
        .page a { padding: 4px 10px; border: 1px solid #cce6f5; margin: 0 4px; color: #198dd2; }
        .page a:hover { background: #198dd2; color: #fff; border: 1px solid #198dd2; }
        ul.page { height: 30px !important; padding-top: 0 !important; }
        .contentcontainer { margin-top: 0px !important; }
        #dropdown > a { text-decoration: underline; font-size: 16px; color: #fff; margin-right: 20px; padding: 5px; }
        .layui-layer-shade { z-index: 1000000 !important; }
        .layui-layer-dialog { z-index: 1000001 !important; }
        .nobr { white-space: nowrap; }
        [v-cloak] { display: none; }
    </style>
    <script>
        //跳页
        $(function () {
            $(document).on("keydown", "input[custompage]", function (event) {
                var url = $(this).attr("address");
                var page = $(this).val();
                if (event.keyCode == 13) { window.location = url.replace("replace", page) }

            })
        });
    </script>
    <script>
        $(function () {
            $('#dropdown').mouseover(function () {
                $(this).addClass('open');
            }).mouseout(function () { $(this).removeClass('open'); });
        })
    </script>
    @if (ViewBag.PageType == 12 || ViewBag.PageType == 22)
