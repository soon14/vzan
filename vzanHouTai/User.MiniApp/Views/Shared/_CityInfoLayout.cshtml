<body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white fontFamilyYahei">
    <div class="page-wrapper">
        <div class="header" style="margin-bottom:0;">
            <div class="headerIn">
                <div class="h_left">
                    @if (ViewBag.PageType > 1)
                    {
                        <a href="@System.Configuration.ConfigurationManager.AppSettings["MiniappContent"]/member/myworkbench"><img src="@WebSiteConfig.SourceContent/content/images/myworkbenchImg/01.png" alt=""></a>
                    }

                </div>
                <div class="dropdown" id="dropdown">
                    @if (ViewBag.PageType > 1)
                    {
                        <a href="javascript:;">管理后台</a>
                        <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                            @*<li><a href="@System.Configuration.ConfigurationManager.AppSettings["DomainCity"]/cityinfo/index" title="旧版管理后台">旧版管理后台</a></li>*@
                            <li role="separator" class="divider"></li>
                            @*<li><a href="@System.Configuration.ConfigurationManager.AppSettings["MiniappContent"]/member/myworkbench" title="新版管理后台">新版管理后台</a></li>*@
                            <li><a href="/dzhome/CaseTemplate" title="新版管理后台">新版管理后台</a></li>
                        </ul>
                    }
                </div>
                <div class="h_right">
                    <span style="color:#fff;margin-right:20px; font-size: 15px;">@ViewBag.username</span>
                    <a href="/dzhome/logout">退出登陆</a>
                    @*<a href="@System.Configuration.ConfigurationManager.AppSettings["MiniappContent"]/home/logout">退出登陆</a>*@
                </div>
            </div>
        </div>
        @*<div class="page-header navbar ">
                    @RenderPage("~/Views/Shared/CityInfo/_TopNav.cshtml")
            @RenderPage("~/Views/Shared/CityInfo/_MenuNav.cshtml")
                </div>*@
        <div class="page-container contentcontainer">
            @if (ViewBag.PageType == 1)
            {
                @RenderPage("~/Views/Shared/officialweb/_SliderNav.cshtml")
            }
            else if (ViewBag.PageType == 4)
            {
                @RenderPage("~/Views/Shared/pageindex/_SliderNav.cshtml")
            }
            else if (ViewBag.PageType == 6)
            {
                @RenderPage("~/Views/Shared/stores/_SliderNav.cshtml")
            }
            else if (ViewBag.PageType == 8)
            {
                @RenderPage("~/Views/Shared/foods/_SliderNav.cshtml")
            }
            else if (ViewBag.PageType == 12)
            {
                @RenderPage("~/Views/Shared/enterprise/_SliderNav.cshtml")
            }
            else if (ViewBag.PageType == 13)
            {
                @RenderPage("~/Views/Shared/gw/_SliderNav.cshtml")
            }
            else if (ViewBag.PageType == 22)
            {
                @RenderPage("~/Views/Shared/enterprisepro/_SliderNav.cshtml")
            }
            else if (ViewBag.PageType == 25)
            {
                @RenderPage("~/Views/Shared/footbath/_SliderNav.cshtml")
            }
            else
            {
                @RenderPage("~/Views/Shared/CityInfo/_SliderNav.cshtml")
            }

            <div class="page-content-wrapper">
                <div class="page-content">

                    @if (ViewBag.PageType != 12 && ViewBag.PageType != 22)
                    {
                        <ol class="breadcrumb">
                            @*<li><a href="/CityInfo/Index">微赞同城</a></li>*@
                            @if (ViewBag.PageType > 1)
                            {
                                <li><a href="/Miapp/index" class="list-city"></a></li>
                            }
                            else
                            {
                                <li><a href="/@(ViewBag.PageType == 1?"Industry":"CityInfo")/Config?cityInfoId=@(ViewBag.cityInfoId)" class="list-city"></a></li>
                            }

                            <li class="active">@ViewBag.Title</li>
                        </ol>
                    }
                    @RenderBody()
                </div>
            </div>
        </div>
        @RenderPage("~/Views/Shared/CityInfo/_FooterNav.cshtml")
    </div>
