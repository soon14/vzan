    <script>

        var array = []
        var array1 = []
        var vm = new Vue({
            el: "#templateList",
            data: {
                pageIndex: 1,
                pagesize: 10,
                templatelist: [],
            },
            methods: {
                gettemplatelist: function () {
                    var list = this;
                    $.ajax({
                        url: "http://testwtApi.vzan.com/apiMiniAppGw/RangeList",
                        type: "get",
                        obj: {},
                        dataType: "json",
                        success: function (res) {
                            if (res.isok == 1) {
                                array = []
                                if (res.obj != undefined && res.obj != null && res.obj.length > 0) {
                                    for (var i = 0; i < res.obj.length; i++) {
                                        var model = res.obj[i]
                                        array.push({ title: model.Title, logo: model.Logo, rangeid: model.RangeId, viewnumbers: model.ViewNumbers })
                                    }
                                }

                                list.templatelist = array;


                            }
                            else {
                                alert(res.msg)
                            }
                        }
                    });
                }
            },
            created: function () {
                $("#templateList").show();
                this.gettemplatelist();
            }


        })
    </script>


