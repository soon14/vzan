

.hidden {
  width: 100%;
  height: 100vh;
  overflow: hidden;
}

.select-model {
  height: 120rpx;
}

.select-show {
  width: 400rpx;
  border-radius: 30rpx;
  border: 2rpx solid #fff;
}

.select-show>text {
  display: block;
  width: 200rpx;
  text-align: center;
}

.select-none {
  background: #ff8929;
  color: #fff;
  border-radius: 30rpx;
}

.select {
  background: #fff;
  color: #ff8929;
  border-radius: 30rpx;
}

.locatio-choose {
  width: 140rpx;
  border-radius: 10rpx;
}

.noStoreAddress {
  width: 400rpx;
  height: 400rpx;
  margin: 150rpx;
}

.noStore {
  width: 300rpx;
  height: 300rpx;
  margin: 150rpx;
}

.loc-btn {
  position: fixed;
  bottom: 0;
  left: 0;
}
