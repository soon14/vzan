/* 顶部兰状态 */

.header {
  width: 100%;
  z-index: 2;
  position: fixed;
  left: 0;
  top: 0;
  background: #ff8929;
  color: #fff;
}

.head-let {
  width: 22%;
  margin-top: 25rpx;
}

.head-min {
  margin-top: 25rpx;
  width: 56%;
}

/* banner */

swiper {
  height: 400rpx;
}

.slide-image {
  width: 100%;
  height: 400rpx;
}

/* label */

.pla-list>text {
  margin-right: 5rpx;
  margin-bottom: 5rpx;
  background: #f2f2f2;
  color: #bfbfbf;
  border-radius: 6rpx;
}

.price::before {
  content: "￥";
  font-size: 22rpx;
}

.priceAll::before {
  content: "小计：";
  font-size: 28rpx;
  color: #121212;
}

/* 选中 */

.select {
  background: #ff953e;
  color: #fff;
}

.select-none {
  background: #f2f2f2;
  color: #121212;
}

/* 购物车 */

.shop-popup {
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 999;
}

.shop-bg {
  height: 100vh;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
}

.scroll {
  margin-top: 30rpx;
}

.shop-dialog {
  border-top: 2rpx solid #f7f7f7;
}

.shop-delete {
  position: absolute;
  top: 10rpx;
  right: 20rpx;
}

.shop-dialog>image {
  width: 200rpx;
  height: 200rpx;
  position: absolute;
  top: -70rpx;
  left: 30rpx;
  border: 5rpx solid #fff;
}

.shop-specinfo {
  margin-left: 250rpx;
}

.select-item>text {
  width: 18%;
  height: 56rpx;
  line-height: 56rpx;
  border-radius: 28rpx;
  text-align: center;
  margin: 10rpx 10rpx 0 0;
}

.num-control>input {
  width: 78rpx;
  height: 58rpx;
  text-align: center;
  background: #ebebeb;
  color: #121212;
}

.num-control>image {
  width: 58rpx;
  height: 58rpx;
}

.add-shopcar {
  position: fixed;
  bottom: 0;
  left: 0;
}

/* 产品信息 */

.pro-item {
  width: 100%;
  border-bottom: 1rpx solid #f5f5f5;
}

.pro-item>image {
  width: 25%;
  height: 172rpx;
}

.pro-content {
  width: 75%;
  margin-left: 20rpx;
  justify-content: space-between;
}

.pro-down {
  justify-content: space-between;
}

/* 无库存弹窗 */

.stock-popup {
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2;
}

.stock-bg {
  width: 100%;
  height: 100vh;
  background-color: #000;
  opacity: 0.4;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1;
}
