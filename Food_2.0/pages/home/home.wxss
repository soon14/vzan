/* pages/home/home.wxss */

page {
  background-color: #f2f2f2;
  font-family: PingFang-SC-Medium;
}

.a {
  margin-top: 34rpx;
}

.b {
  width: 218rpx;
  height: 142rpx;
}

.c {
  max-width: 80rpx;
  margin-bottom: 12rpx;
}

.d {
  height: 200rpx;
  width: 300rpx;
}

.canvas_bottom_btn {
  position: relative;
  bottom: -30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  z-index: 3;
}

.canvas_save {
  display: flex;
  flex-direction: column;
  align-items: center;
  z-index: 3;
}

.canvas_img {
  width: 96rpx;
  height: 96rpx;
  background: #fff;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 5rpx;
}

.share_Canvas {
  position: relative;
  left: 8%;
  width: 650rpx;
  height: 100vh;
  display: block;
  margin: 0 auto;
}

.share_Canvas_none {
  display: none;
}

.share_Canvas canvas {
  z-index: 2;
}

.Canvas_cancle {
  position: relative;
  left: 90%;
  z-index: 3;
}