/* pages/orderList/orderList.wxss */

page {
  font-family: PingFang-SC-Medium;
}

.all {
  display: flex;
  flex-direction: column;
  margin-left: 27rpx;
border-bottom: 1rpx solid #e6e6e6;  
  padding: 36rpx 24rpx 30rpx 0;
}

.eatcondition {
  line-height: 30rpx;
  text-align: center;
  margin-left: 11rpx;
  width: 60rpx;
  height: 30rpx;
  font-size: 22rpx;
  background: #fab95d;
  color: white;
  border-radius: 4rpx;
}

.line1 {
  font-size: 32rpx;
  justify-content: space-between;
  display: flex;
}

.orderinfo {
  display: flex;
  font-size: 28rpx;
  color: #666;
}
.line4{
  display: flex;
  justify-content: flex-end;
}
.line4btn{
  margin-top: 39rpx;
  border-radius: 4rpx;
  border: 1rpx solid #e6e6e6;
  width: 122rpx;
  height: 40rpx;
  font-size: 24rpx;
  text-align: center;
  line-height: 40rpx;
  color: #666;
}
