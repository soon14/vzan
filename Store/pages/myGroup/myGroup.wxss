page{background-color: #f7f7f7;}
.header .item{text-align: center;height: 100rpx;line-height: 100rpx;}
.header .item.sel {color: #F20033;}

.groupUnit{padding: 10px;background-color: #fff;}
.groupUnit .pimg{width:250rpx;height: 250rpx;background-color: red;vertical-align: middle;}
.groupNumDes{color: #FFB049 ;margin-top: 10px;}
.groupNumDes .groupnum{border: 1rpx solid #FFB049;padding: 2rpx 16rpx;border-radius: 20rpx;margin-right: 20rpx;}
.btn-startGroup{width:120rpx;height: 50rpx;text-align: center;line-height: 50rpx;background-color:#F20033;color: #fff;font-size: 28rpx;border-radius: 8rpx; }
.priceDesc .f34,
.priceDesc .f22
{color: #F20033;}
.priceDesc .price2{text-decoration: line-through;}

.myGroupUnit{margin: 0 20rpx 20rpx 20rpx;padding: 20rpx;border-radius: 10rpx;}
.myGroupUnit .pimg{width: 200rpx;height: 200rpx;vertical-align: middle;}
.myGroupUnit .orderType{width: 60rpx;height: 30rpx;position: absolute;top: 0;left: 0;font-size: 20rpx;color: #fff;background-color: #F20033;text-align:center;line-height: 30rpx;}
.myGroupUnit .pname{width: 100%;max-width: 350rpx;}
.orderState{color:#F20033;font-size: 26rpx;}
.groupState{color:#FFB049;}
.btns{float: right;}
.btn{padding:14rpx 20rpx; border-radius: 10rpx;font-size: 22rpx;text-align: center;color: #999;border: 1rpx solid #999;vertical-align: middle;}
.btn-primary{background-color: #F20033;color: #fff;border: 0;}
