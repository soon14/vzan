/*表单组件 begin*/
    .c-form { background-color: #fff; }
    .c-form .c-form-title { font-size: 16px; padding: 15px 0; border-bottom: 1px solid #f7f7f7; line-height: 1;text-align: center; }

    .c-form.formstyle1 .c-form-title text:before { content: ""; position: absolute; top: 50%; transform: translateY(-50%); width: 30px; height: 1px; background-color: #000; left: -40px; }
    .c-form.formstyle1 .c-form-title text:after { content: ""; position: absolute; top: 50%; transform: translateY(-50%); width: 30px; height: 1px; background-color: #000; right: -40px; }


    .c-form.formstyle2 .c-form-title text:before,
    .c-form.formstyle2 .c-form-title text:after,
    .c-form.formstyle3 .c-form-title text:before,
    .c-form.formstyle3 .c-form-title text:after { display: none; }
    .c-form-label { background-color: #fff; font-size: 14px; color: #333; font-weight: bold; padding: 0; line-height: 1; margin-top: 20px; }
    .c-form-input { padding: 10px 0; height: auto; line-height: 1; }
    .c-form-wrap { padding: 20px; padding-top: 0; }

    .c-form .c-form-control { padding-left: 0; height: auto; line-height: 1; }
    .c-form .weui-cells:before,
    .c-form .weui-cells:after { display: none; }
    .c-form.formstyle2 .weui-cells:before { left: 0; }
    .c-form .weui-cells_radio { margin-top: 10px; border: 1px solid #e5e5e5; }
    .c-form .weui-cell { height: 50px; line-height: 50px; font-size: 12px; border-top: 1px solid #e5e5e5; }
    .c-form .weui-cell:first-child { border-top: 0; }
    .c-form .weui-cell:before { left: 0; display: none; }
    .c-form-submit { width: 100%; height: 44px; line-height: 44px; text-align: center; margin-top: 30px; font-size: 28rpx;}

    .c-form.formstyle1 .c-form-input { border-bottom: 1px solid #dcdcdc; }

    .c-form.formstyle2 .c-form-label { padding-left: 10px; }
    .c-form.formstyle2 .c-form-input { background-color: #f7f7f7; color: #999999; border-radius: 5px; margin-top: 10px;padding-left: 10px; }
    .c-form.formstyle2 .c-form-submit { border-radius: 5px; }
    .c-form.formstyle2 .weui-cells_radio { border-radius: 5px; }


    .c-form.formstyle3 .c-form-label { padding-left: 10px; }
    .c-form.formstyle3 .c-form-input { border: 1px solid #dcdcdc; margin-top: 10px; padding-left: 10px; }

  
