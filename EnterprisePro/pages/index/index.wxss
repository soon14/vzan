/* pages/index/index.wxss */
@import "/template/template.wxss";
@import "/css/group2.0.wxss";
/*flex头部*/

.sc-top {
  display: -webkit-box;
  border-bottom: 1rpx solid #f5f5f5;
}

.scroll-page {
  width: 100%;
  height: 100vh;
  position: absolute;
  top: 0;
  left: 0;
}

/*flex内容区*/

.scroll-content {
  width: 100%;
  height: 100vh;
  position: relative;
  /* -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  overflow-y: auto;
  -webkit-overflow-scrolling: touch; */
}

/* 表单名称颜色渐变 */

.agents {
  background: -webkit-linear-gradient(left top, #ff5a9b, #873aef);
  -webkit-background-clip: text;
  color: transparent;
}
.pro-sel{background-color: #fff;}