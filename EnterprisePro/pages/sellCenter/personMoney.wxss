/* pages/sellCenter/personMoney.wxss */
@import "/template/template.wxss";

.moneyIcon {
  width: 294rpx;
  height: 204rpx;
}

.btn {
  width: 100%;
  padding: 0 20rpx;
}

.btn>view {
  border-radius: 8rpx;
}

.mt80 {
  margin-top: 80rpx;
}

.mh50 {
  margin: 50rpx 0;
}

.mt40 {
  margin-top: 40rpx;
}

.user {
  width: 100%;
  height: 180rpx;
  margin: 20rpx 0;
  padding: 0 20rpx;
  background: #fff;
}

.user>image {
  width: 130rpx;
  height: 130rpx;
  margin-right: 20rpx;
  border-radius: 50%;
}

.in-price {
  width: 100%;
  height: 180rpx;
  padding: 20rpx;
  background: #fff;
}

.account {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background: #fff;
  padding: 0 20rpx;
  border-top: 1rpx solid #f3f3f3;
}

.accountBtn {
  width: 100%;
  padding: 0 20rpx;
  margin-top: 60rpx;
}
.accountBtn>view{
  border-radius: 8rpx;
}