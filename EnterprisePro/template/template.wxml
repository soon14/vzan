<!-- Bargain -->
<template name="barTemp">
  <block wx:if="{{this.type=='cutprice'}}">
    <block wx:for='{{listviewModel_cutprice_arr[isIndex1+ "_"+comindex].list}}'>
      <view class='mt20 bgff p-re ' data-id="{{item.Id}}" data-loadingT="{{item.loadingT}}" catchtap='templGoto' id="8">
        <view class='rel'>
          <image src='{{item.ImgUrl_thumb}}' class='w100 h420'></image>
          <view class='b-line w100 h120nl abs' style='bottom:0;width:100%;'>
            <view class='abs w100 h120nl' style='z-index:1;'>
              <image src='http://j.vzan.cc/miniapp/img/enterprise/a44.png' class='w100 h120nl' wx:if="{{a.toTime(item,nowTime).isbegin==false&&a.toTime(item,nowTime).isend==false}}"></image>
              <image src='http://j.vzan.cc/miniapp/img/enterprise/a42.png' class='w100 h120nl' wx:if="{{a.toTime(item,nowTime).isbegin==true&&a.toTime(item,nowTime).isend==false&&item.RemainNum!=0}}"></image>
              <image src='http://j.vzan.cc/miniapp/img/enterprise/a43.png' class='w100 h120nl' wx:if="{{a.toTime(item,nowTime).isbegin==true&&a.toTime(item,nowTime).isend==false&&item.RemainNum==0}}"></image>
              <image src='http://j.vzan.cc/miniapp/img/enterprise/a43.png' class='w100 h120nl' wx:if="{{a.toTime(item,nowTime).isbegin==false&&a.toTime(item,nowTime).isend==true}}"></image>
            </view>

            <view class='abs w100 h120nl f fc cfff p20' style='z-index:2'>
              <view class='f1'>
                <view>
                  <text>最低价:</text>
                  <text class='pr-price f38'>{{item.FloorPriceStr}}</text>
                  <text class='pr-price m20-l t-del'>{{item.OriginalPriceStr}}</text>
                </view>
                <text class='f24'>剩余{{item.RemainNum}}份</text>
              </view>

              <view class=''>
                <view wx:if="{{a.toTime(item,nowTime).isbegin==false&&a.toTime(item,nowTime).isend==false}}">距离开始</view>
                <view wx:if="{{a.toTime(item,nowTime).isbegin==true&&a.toTime(item,nowTime).isend==false&&item.RemainNum!=0}}">距离结束</view>
                <view wx:if="{{a.toTime(item,nowTime).isbegin==true&&a.toTime(item,nowTime).isend==false&&item.RemainNum==0}}">活动结束</view>
                <view wx:if="{{a.toTime(item,nowTime).isbegin==false&&a.toTime(item,nowTime).isend==true}}">活动结束</view>
                <view wx:if="{{a.toTime(item,nowTime).isbegin==true&&a.toTime(item,nowTime).isend==false&&item.RemainNum!=0||a.toTime(item,nowTime).isbegin==false&&a.toTime(item,nowTime).isend==false&&item.RemainNum!=0}}" class='f24'>{{a.toTime(item,nowTime).time}}</view>
                <view wx:if="{{a.toTime(item,nowTime).isbegin==false&&a.toTime(item,nowTime).isend==true||item.RemainNum==0}}" class='f24'>00:00:00</view>
              </view>
            </view>
          </view>

        </view>
        <view class='p20 bgff f fc'>
          <view class='f f1'>{{item.BName}}</view>
          <view class='w150nl bgf2 center h60-l cfff fs0' wx:if="{{a.toTime(item,nowTime).isbegin==true&&a.toTime(item,nowTime).isend==false&&item.RemainNum!=0}}" catchtap="templGoto" data-id="{{item.Id}}" id="8">立即砍价</view>
          <view class='w150nl bge6 center h60-l cfff fs0' wx:if="{{a.toTime(item,nowTime).isbegin==true&&a.toTime(item,nowTime).isend==false&&item.RemainNum==0}}" catchtap="showTimeout">活动结束</view>
          <view class='w150nl bge6 center h60-l cfff fs0' wx:if="{{a.toTime(item,nowTime).isbegin==false&&a.toTime(item,nowTime).isend==true}}" catchtap="showTimeout">活动结束</view>
          <view class='w150nl bge6 center h60-l cfff fs0' wx:if="{{a.toTime(item,nowTime).isbegin==false&&a.toTime(item,nowTime).isend==false}}" catchtap="showLoading">立即砍价</view>
        </view>


      </view>
    </block>
  </block>

</template>
