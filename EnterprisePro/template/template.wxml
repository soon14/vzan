<!-- map -->
<template name="mapTemp">
  <block wx:if="{{this.type=='map'}}">
    <view class='flex-r-c-sb p20-w bgff f30 h100-l' bindtap="pageFunc" data-lat="{{this.latitude}}" data-lng='{{this.longitude}}' id="0">
      <view class='flex-r-c-sb'>
