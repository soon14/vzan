          <view wx:if="{{item.img.length==''}}" class='w100 h360nl bgcc news-item-blankimg{{this.liststyle}} br10'></view>
          <image mode='aspectFill' wx:else="{{item.img.length!=''}}" class='w100 h360nl br10' src='{{item.img_fmt}}'></image>
