    </view>
    <!--背景音乐-->
    <view wx:if="{{status._bgmusic && isPlay!= null}}" class='s-phone'>
      <image wx:if="{{isPlay==true}}" style="height: 37rpx; width: 37rpx;" mode="widthFix" src="/image/stopmusic4.png" bindtap='playAudioFunc' data-src='{{this.src}}' data-play="{{this.autoplay}}"></image>
      <image wx:if="{{isPlay==false}}" src='/image/bgmusic1.png' bindtap='stopAudioFunc' style="height: 37rpx; width: 37rpx;" mode="widthFix"></image>
    </view>
  </view>
