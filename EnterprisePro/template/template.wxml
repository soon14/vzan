      <button style="width: 88rpx;height: 88rpx; background: url('http://j.vzan.cc/dz/content/Miniapp/imgs/enterprise/weChatLog_2.png');background-size: cover;border: 0;background-color:none; " class="btn-img" hover-class="none" open-type="contact">
      </button>
    </view>
  </block>
</template>
<!--联系店主拨打电话和客服悬浮标，附上之前的分享，背景音乐-->
<template name="showPhone">
  <view class='showIcon'>
    <!--拨打电话-->
    <view class="s-phone " wx:if="{{status.openTelSuspend}}" bindtap="pageFunc" id="7" data-phone="{{status. contactPhone}}">
      <image style="height: 37rpx; width: 37rpx;" mode="widthFix" src="/image/phone2.png"></image>
    </view>
    <!--客服-->
    <view class='s-phone' wx:if="{{status.openServiceSuspend}}">
