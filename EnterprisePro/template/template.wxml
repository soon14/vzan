<!-- banner -->
<template name="banTemp">
  <block wx:if="{{this.type=='slider'}}" wx:for-item="this">
    <swiper autoplay="tue" interval="5000" duration="500" style='height:400rpx;' indicator-dots="{{true}}">
      <block wx:for="{{this.items}}">
        <swiper-item>
          <image mode='scaleToFill' src="{{item.img_fmt}}" class="w100 h400nl" bindtap='goNewPage' data-id="{{item.url}}" data-type="{{this.type}}" data-items="{{this.items}}" id="{{index}}" />
        </swiper-item>
      </block>
    </swiper>
  </block>
</template>
<!--search-->
<template name="search">
  <navigator open-type="navigate" hover-class="none" url="../goodsSearch/goodsSearch">
    <view wx:if="{{this.type=='search'}}" class="c-search {{'searchbg'+this.bgStyle}}">
      <view class="{{'search'+this.style}}">
        <view class="searchinput">
          <text class='dzicon icon-sousuo'></text>
          <text class="placeholder"> 商品搜索</text>
        </view>
      </view>
    </view>
  </navigator>
</template>
<!--spacing-->
<template name="spacing">
  <block wx:if="{{this.type == 'spacing'}}">
    <view class="spacing" style="height: {{this.spacing + 'px'}}"></view>
  </block>
</template>

<!--contactShopkeeper 联系店主-->
<template name="contactShopkeeper">
  <block wx:if="{{this.type == 'contactShopkeeper'}}">
    <view class="c-contactShopkeeper">
      <view class="l-contactShopkeeper">
        <view class="shopimg">
          <image style="height: 100rpx; width: 100rpx; border-radius: 10rpx;" mode="widthFix" src="{{this.shopLogo_url}}"></image>
        </view>
        <view class="shoptext">
          <text class="topDoc">欢迎光临本店，我是店主</text>
          <text class="bottomDoc">点击右侧按钮立即咨询店主→→→</text>
        </view>
      </view>
      <view class="r-contactShopkeeper">

        <view class="p-img " wx:if="{{this.openTel}}" bindtap="pageFunc" id="7" data-phone="{{this.phoneNum}}">
          <image style="height: 70rpx; width: 70rpx;" mode="widthFix" src="/image/phone1.png"></image>
        </view>
        <view class="p-img2" wx:if="{{this.openService}}" bindtap="">
          <button style="width: 70rpx;height: 70rpx; background: url('http://j.vzan.cc/dz/content/images/Enterprisepro/weChat_new.png');background-size: cover;border: 0;background-color:none; " class="btn-img" hover-class="none" open-type="contact"></button>
        </view>


      </view>
    </view>
  </block>
</template>

<!--商品详情弹框推送-->
<template name="shopMessage">
  <block wx:if="{{popinfo.openWxShopMessage && !popinfo.modalClose }}">
    <view class="r-shopMessage">
      <view class="topPart">
        <view class="t-img">
          <image style="height: 40rpx; width: 40rpx; border-radius: 10rpx;" mode="scaleToFill" src="{{popinfo.headImg}}"></image>
        </view>
        <view class='t-doc'>{{popinfo.nickName}}</view>
        <text class='t-txt'>刚刚</text>
      </view>

      <view class="bottomPart">
        <view class="b-txt left-color">店主给你发来一条信息,</view>
        <button class="right-color" style="height: 35rpx;" open-type='contact'>点击查看</button>
        <view class="b-img" bindtap='modalClose'>
          <image style="height: 24rpx; width: 24rpx;" mode="widthFix" src="/image/close.png"></image>
        </view>
      </view>
    </view>
  </block>
</template>



<!--客服悬浮标 附上详情推送框-->
<template name="showWechat">
  <block wx:if="{{popinfo.openWxShopMessage && !popinfo.modalClose}}">
