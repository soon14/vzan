<!--只有图片，例如图片展示；item_image-->
<template name="item_image">
  <scroll-view scroll-x="true" class="item_image">
    <view class="images-list">
      <block wx:for="{{content.imageList}}" wx:for-item="image" wx:for-index="imageIndex">
        <view class="images-image">
          <image class="image" src="{{image}}" data-src="{{image}}" bindtap="previewImage" data-row="{{index}}" data-index="{{imageIndex}}"></image>
          <image src="../../image/delete.png" class="clear_btn" bindtap="clearImage" data-row="{{index}}" data-index="{{imageIndex}}"></image>
        </view>
      </block>
      <view class="images-image" bindtap="chooseImage" wx:if="{{!content.images_full}}" data-row="{{index}}" data-index="{{imageIndex}}" style="padding-right: 30rpx;background-color:#f6f6f6">
        <image class="image" src="../../image/addImage.png" data-src="{{image}}" bindtap="" data-row="{{index}}" data-index="{{imageIndex}}"></image>
      </view>
    </view>
  </scroll-view>
</template>

<!--图片 + 标题-->
<template name="item_image_title">
  <view class="item_image">
    <view class="images-list" style="background-color : white ;padding: 24rpx 30rpx 24rpx 30rpx;">
      <block wx:for="{{content.imageList}}" wx:for-item="image" wx:for-index="imageIndex">
        <image src="{{image == '' ? '../../image/placeholderImage.png' : image}}" class="images-image" style="margin:20rpx 0rpx 20rpx 0rpx" data-src="{{image}}" bindtap="chooseImage" data-row="{{index}}" data-index="{{imageIndex}}" data-rechoose="true"></image>
      </block>
      <view class="images-image" style="margin: 0rpx;" bindtap="chooseImage" wx:if="{{!content.images_full}}" data-row="{{index}}" data-index="{{imageIndex}}" style="margin:20rpx 0rpx 20rpx 0rpx">
        <image class="image" src="../../image/placeholderImage.png" data-src="{{image}}" data-row="{{index}}" data-index="{{imageIndex}}" data-rechoose="true"></image>
      </view>
      <view class="titleView" style="margin:20rpx 0rpx 20rpx 20rpx">
        <text class="text">{{content.title}}</text>
      </view>
    </view>
  </view>
</template>

<!--内容 + 图片-->
<template name="item_content_image">
  <view class="item_content_image">
    <textarea class="input" placeholder="{{content.placeholder}}" value="{{content.value}}" bindinput="item_content_image_save" data-row="{{index}}" />
    <scroll-view scroll-x="true" class="item_image">
      <view class="images-list" style="background-color: {{content.hasBgColor?'white':''}}">
        <block wx:for="{{content.imageList}}" wx:for-item="image" wx:for-index="imageIndex">
          <view class="images-image">
            <image class="image" src="{{image}}" data-src="{{image}}" bindtap="previewImage" data-row="{{index}}" data-index="{{imageIndex}}"></image>
            <image src="../../image/delete.png" class="clear_btn" bindtap="clearImage" data-row="{{index}}" data-index="{{imageIndex}}"></image>
          </view>
        </block>
        <view class="images-image" bindtap="chooseImage" wx:if="{{!content.images_full}}" data-row="{{index}}" data-index="{{imageIndex}}" style="padding-right: 30rpx;background-color:rgba(0,0,0,0)">
          <image class="image" src="../../image/addImage_white.png" data-src="{{image}}" bindtap="" data-row="{{index}}" data-index="{{imageIndex}}"></image>
        </view>
      </view>
    </scroll-view>
  </view>
</template>

<!-- Text -->
<template name="item_text">
  <view name="{{index}}" class="template_item_view flex template_flex_box" style="justify-content: flex-start;align-items: center">
    <text class="f-15rem">{{content.value}}</text>
  </view>
</template>

<!--内容-->
<template name="item_content">
  <view class="item_content">
    <textarea name="{{index}}" class="input" placeholder="{{content.placeholder}}" value="{{content.value}}" bindinput="item_content_save" data-row="{{index}}" />
  </view>
</template>

<!--标题 + 内容-->
<template name="item_title_content">
  <view class="item_title_content">
    <view class="title">{{content.title}}</view>
    <input name="{{index}}" class="input" type="{{content.inputType ? content.inputType : 'text'}}" placeholder="{{content.placeholder}}" value="{{content.value}}" disabled="{{!content.canTap}}" maxlength="{{content.maxlength}}" bindinput="item_title_content_save"
      data-row="{{index}}" />
  </view>
</template>
<!--内容+按钮-->
<template name="item_content_button">
  <view class="item_content_button">
    <button class="{{content.changebak}}" style="font-size:30rpx;margin-left:5px;margin-right:0px;height:88rpx;line-height:88rpx;width:115px;color:white;float:right;" catchtap="getCode">{{content.changecontent}}</button>
    <input name="{{index}}" class="input" type="{{content.inputType ? content.inputType : 'text'}}" placeholder="{{content.placeholder}}" disabled="{{!content.canTap}}" maxlength="{{content.maxlength}}" bindinput="item_ontent_botton_save" data-row="{{index}}"
    />

  </view>
</template>

<!--标题 + 下拉列表-->
<template name="item_title_picker">
  <view class="item_title_picker">
    <view class="title">{{content.title}}</view>
    <picker class="pickerwidth" bindchange="bindPickerChange" value="{{content.listitem[0]}}" range="{{content.listitem}}" data-id="{{index}}">
      <view class="picker">
        {{content.value}}
      </view>
    </picker>
  </view>
</template>

<!--标题 + 星号 + 内容-->
<template name="item_title_star_content">
  <view name="{{index}}" class="item_title_star_content">
    <view>
      <text class="title">{{content.title}}
        <text class="star">*</text>
      </text>

    </view>
    <input name="{{index}}" class="input" type="{{content.inputType ? content.inputType : 'text'}}" placeholder="{{content.placeholder}}" value="{{content.value}}" disabled="{{!content.canTap}}" bindinput="item_title_star_content_save" data-row="{{index}}"
    />
  </view>
</template>
<template name="item_title_record">
  <view class="recordContentView">
    <image src="" class="recordImageView"></image>
  </view>
</template>
