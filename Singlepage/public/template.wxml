              <block wx:if="{{content.AgeQ!=null && content.AgeQ!=''}}">年龄：{{content.AgeQ}}</block>
              <block wx:if="{{content.WorkTime!=null && content.WorkTime!=''}}">学历：{{content.WorkTime}}</block>
            </view>
            <block wx:if="{{content.Experience!=null && content.Experience!='' && content.Experience>=1&& content.Experience<=7}}">
              <view class="hd_name2">
                工作经验：
                <block wx:if="{{content.Experience==1}}">无工作经验</block>
                <block wx:elif="{{content.Experience==2}}">应届毕业生</block>
                <block wx:elif="{{content.Experience==3}}">1年以下工作经验</block>
                <block wx:elif="{{content.Experience==4}}">1 - 3年工作经验</block>
                <block wx:elif="{{content.Experience==5}}">3 - 5年工作经验</block>
                <block wx:elif="{{content.Experience==6}}">5 - 10年工作经验</block>
                <block wx:elif="{{content.Experience==7}}">10年以上工作经验</block>
              </view>
            </block>
            <view class="hd_name2" wx:if="{{content.Salary!=null && content.SalaryName!=null && content.SalaryName!='' && content.Salary!='1'}}">期望月薪：￥{{content.SalaryName}}</view>
          </block>
        </block>
        <!--拼车-->
        <block wx:elif="{{content.PTypeId==304069}}">
          <view class="hd_name2" w>出发时间：{{content.ShowTime}}</view>
          <view class="hd_name2" w>{{content.IdentityType==4?"乘客":"空座"}}：{{content.ValNumber}}</view>
          <view class="hd_name2" wx:if="{{content.IdentityType==3 && content.carPlate!=null && content.carPlate!=''}}">车牌号：{{content.carPlate}}</view>
        </block>
        <view class="hd_name" wx:if="{{content.Description!='' && content.Description!=null}}">详情：{{content.Description}}</view>
      </view>
      <view class="desriptimg">
        <block wx:for="{{content.ImgList}}" wx:for-item="img" wx:for-index="imgindex">
          <image wx:if="{{imgindex<9}}" mode="aspectFit" src="{{img.Thumbnail}}" data-src="{{img.FileFullUrl}}" data-id="{{content.Id}}" bindtap="pictureTap"></image>
        </block>
      </view>
      <view class="hd_c_center" wx:if="{{content.Address!=null && content.Address!=''}}">
        <i style="background: url({{tongcheng_new_02}})no-repeat ;background-size:20rem;background-position-x:-4.95rem;background-position-y:-19.2rem;position:absolute;height:0.7rem;width:15px;display:inline-block;"></i>
        <text class="hd_addres">{{content.Address}}</text>
      </view>
      <view class="hd_c_bot">
        <span class="now_price">{{content.FromNowTime}}</span>
        <span class="hd_s" catchtap="bottomItemClick" data-id="{{content.Id}}" data-type="pl">
                           <i class="hearder_sou_i" style="background: url({{tongcheng_new_02}})no-repeat ;background-size:450px;background-position-x:-63px;background-position-y:-247px;"></i>
                          <text>{{content.CommentCount}}</text>
                        </span>
        <span class="hd_s">
                          <i class="hearder_sou_i" style="background: url({{tongcheng_new_02}})no-repeat ;background-size:450px;background-position-x:-325px;background-position-y:-183px;"></i>
                          <text>{{content.ViewCount}}</text>
                        </span>
      </view>
    </view>
    <i wx:if="{{content.TopStatus>0}}" style="background: url({{tongcheng_01}})no-repeat ;background-size:30rem;background-position-x:0.5rem;background-position-y:-25.3rem;width:2rem;height:2rem;position:absolute;right:0px;top:0px;"></i>
  </view>
</template>


<!-- 时间选择器 -->
<template name="timePicker">
<view class="timePicer">
  <picker-view indicator-style="height: 50px;" style="width: 100%; height: 600rpx;" value="{{value}}" bindchange="bindMultiPickerChange">
    <picker-view-column class="pickerview">
      <view wx:for="{{years}}" class="">{{item}}年</view>
    </picker-view-column>
    <picker-view-column class="pickerview">
      <view wx:for="{{months}}">{{item}}月</view>
    </picker-view-column>
    <picker-view-column class="pickerview">
      <view wx:for="{{days}}">{{item}}日</view>
    </picker-view-column>
    <picker-view-column class="pickerview">
      <view wx:for="{{ours}}">{{item}}时</view>
    </picker-view-column>
    <picker-view-column class="pickerview">
      <view wx:for="{{minues}}">{{item}}分</view>
    </picker-view-column>
  </picker-view>
  <!-- 按钮 -->
  <view class="btn">
    <text style="margin-left:30rpx;color:rgb(138,138,138)" bindtap="timecancel">取消</text>
    <text style="margin-right:30rpx;color:rgb(94,220,87)" bindtap="clickOk">确定</text>
  </view>
</view>
</template>