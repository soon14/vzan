        value="{{content.value2}}" />
    </view>
  </view>
</template>

<!-- 15、	服务时间选择，周排 和 月排             选择按钮-->
<template name="item_serviceTime">
  <view class="template_item_view template_flex_box flex">
    <text class="item_serviceTime_title f-14rem">{{content.title}}</text>
    <view class="item_serviceTime_red_radius">
      <view class="item_serviceTime_child infoTextColor f-12rem {{content.weekSS}}" bindtap="showWeek" data-id="{{index}}">
        {{content.week}}
      </view>
      <view class="item_serviceTime_child infoTextColor f-12rem {{content.monthSS}}" bindtap="showMonth" data-id="{{index}}">
        {{content.month}}
      </view>
    </view>
  </view>
  <view class="flex item_serviceTime_detail">
    <block wx:for="{{content.date}}" wx:for-index="position">
      <text class="item_serviceTime_detail_text contentTextColor f-12rem {{item.select?'item_serviceTime_detail_text_select':''}}" catchtap="onTimeItemClick" data-row="{{index}}" data-id="{{position}}">{{item.content}}
      </text>
    </block>
  </view>
</template>


