<!--标题1&输入框 + 标题2&输入框-->
<template name="item_price_origin_discount">
  <view class="template_item_view template_flex_box flex">
    <view class="item_price_origin_discount">
      <text class="f-15rem">{{content.title1}}</text>
      <input name="{{index}}-first" class="f-15rem" type="number" placeholder="{{content.placeholder1}}" bindinput="item_price_origin_discount_first_save" data-row="{{index}}" value="{{content.value1}}" />
