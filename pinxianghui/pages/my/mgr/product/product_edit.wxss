.edit-list {
  position: fixed;
  top: 6vh;
  width: 100%;
  height: 94vh;
  z-index: 3;
  background-color: #f7f7f7;
  padding-top: 20rpx;
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
}

.edit-list .head {
  text-align: center;
  height: 89rpx;
  line-height: 88rpx;
  border-bottom: 1rpx solid #f7f7f7;
}

.edit-list .head text {
  display: inline-block;
  margin: 0 auto;
  height: 88rpx;
  line-height: 88rpx;
  color: #333;
  font-size: 32rpx;
}

.edit-list .head text.sel {
  border-bottom: 4rpx solid #333;
  font-weight: bolder;
}

.edit-list .edit-list-item {
  height: 110rpx;
  color: #666;
  font-size: 32rpx;
  padding: 0 20rpx;
  border-bottom: 1rpx solid #f7f7f7;
}

.edit-list .edit-list-btn {
  display: inline-block;
  padding: 16rpx 20rpx;
  border-radius: 6rpx;
  margin-left: 20rpx;
}

.edit-list .edit-list-btn.blue {
  color: #53a3fe;
  border: 1rpx solid #53a3fe;
}

.edit-list .edit-list-btn.red {
  color: #ff6565;
  border: 1rpx solid #ff6565;
}

