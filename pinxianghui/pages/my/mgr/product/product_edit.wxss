}

.attr-selected .btn-auto {
  padding: 20rpx 27rpx;
  min-width: 180rpx;
  text-align: center;
  background-color: #fe536f;
  color: #fff;
  font-size: 28rpx;
  border-radius: 6rpx;
  margin-right: 20rpx;
  display: inline-block;
  vertical-align: middle;
}

